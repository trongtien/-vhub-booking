{
  "name": "vhub-booking",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:fe": "turbo run dev --filter=@booking/shell",
    "build:be:lib": "turbo run build --filter=@booking/serve-identity ",
    "dev:be": "turbo run dev --filter=@booking/serve-main",
    "dev:sever-main": "turbo run dev --filter=@booking/serve-main ",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@booking/eslint-config": "workspace:*",
    "@booking/typescript-config": "workspace:*",
    "@types/jest": "^29.5.2",
    "@types/node": "^22.15.3",
    "eslint": "^9.39.1",
    "jest": "^29.5.0",
    "knex": "^3.1.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "turbo": "^2.7.3",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "zod": "^4.2.1"
  }
}
