    "migrate:make": "knex-cli-make --path=./migrations",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@booking/serve-core": "workspace:*",
    "@booking/serve-knex-cli": "workspace:*"
  },
  "devDependencies": {
    "@booking/eslint-config": "workspace:*",
    "@booking/jest-config": "workspace:*",
    "@booking/typescript-config": "workspace:*",
