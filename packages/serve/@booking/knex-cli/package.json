{
  "name": "@booking/serve-knex-cli",
  "version": "0.0.1",
  "description": "Knex CLI tools for database migrations",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli-exports.d.ts",
      "default": "./dist/cli-exports.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "knex-cli-migrate": "./dist/cli-migrate.js",
    "knex-cli-rollback": "./dist/cli-rollback.js",
    "knex-cli-make": "./dist/cli-make-file.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.json --watch",
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@booking/eslint-config": "workspace:*",
    "@booking/jest-config": "workspace:*",
    "@booking/typescript-config": "workspace:*"
  }
}
