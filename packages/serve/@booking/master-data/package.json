{
  "name": "@booking/serve-master",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --max-warnings 0",
    "migrate": "ts-node -r tsconfig-paths/register ../knex-cli/src/cli-migrate.ts --folder=./migrations --path-env=../../../../.env.local",
    "migrate:rollback": "ts-node -r tsconfig-paths/register ../knex-cli/src/cli-rollback.ts --folder=./migrations --path-env=../../../../.env.local",
    "migrate:make": "ts-node -r tsconfig-paths/register ../knex-cli/src/cli-make-file.ts --path=./migrations",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@booking/serve-core": "workspace:*",
    "@booking/serve-knex-cli": "workspace:*"
  },
  "devDependencies": {
    "@booking/eslint-config": "workspace:*",
    "@booking/jest-config": "workspace:*",
    "@booking/typescript-config": "workspace:*",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "ts-jest": "^29.1.2"
  }
}
